<?xml version="1.0"?>
 <launch>
  <!-- send table urdf to param server -->
<arg name="object"/>
<param name="object_description" command="$(find xacro)/xacro $(find araig_objects)/objects/$(arg object).urdf.xacro" />

 <!-- push table_description to factory and spawn robot in gazebo -->
 <node name="spawn_$(arg object)" pkg="gazebo_ros" type="spawn_model" args="-urdf -param object_description -z 0.01 -model $(arg object)" respawn="false" output="screen" />
</launch>
